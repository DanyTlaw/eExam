<p id="notice"><%= notice %></p>

<h1>Course Of Studies</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @course_of_studies.each do |course_of_study| %>
      <tr>
        <td><%= link_to 'Show', course_of_study %></td>
        <td><%= link_to 'Edit', edit_course_of_study_path(course_of_study) %></td>
        <td><%= link_to 'Destroy', course_of_study, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<div ng-controller="TestController">
  <button ng-click="testf()">test</button>
  {{entries}}
  <ul>
    <li ng-repeat="entry in entries">
      {{entry.check}}
    </li>
  </ul>
</div>

<%= link_to 'New Course Of Study', new_course_of_study_path %>
