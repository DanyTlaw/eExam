<p id="notice"><%= notice %></p>

<div class="panel panel-primary ">
  <div class="panel-heading">Courses</div>
  <div class="panel-body">
    <table class="table">
    <th>Email</th>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.email %></td>
        <td><%= link_to 'Anzeigen', course_of_study_course_student_path(@course_of_study.id,@course.id, student.id), class: "btn btn-info" %></td>
      </tr>
    <% end %>
    </table>
  </div>
</div>

<br>

<%= link_to 'Dashboard', {:controller => "students", :action => "dashboard", :course_of_study_id => @course_of_study, :course_id => @course}, "data-no-turbolink" => true, class:"btn btn-primary" %>
